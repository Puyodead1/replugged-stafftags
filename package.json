{
  "name": "replugged-stafftags",
  "version": "1.0.19",
  "description": "Adds a tag or crown to Owners, Mods, Admins and Staff.",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "replugged build plugin",
    "watch": "replugged build plugin --watch",
    "bundle": "replugged bundle plugin",
